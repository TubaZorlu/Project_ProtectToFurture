							<div class="col-sm-3">
								<div class="abt-icon" data-blast="bgColor">
									<span class="fa fa-ravelry"></span>
								</div>
							</div>
							<div class="col-sm-9 pl-sm-0">
								<div class="abt-txt ml-sm-0">

									<p>
										@Model.Description 
									</p>
								</div>
							</div>
